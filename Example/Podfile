use_frameworks!

target 'JFWindguruPure_Example' do
  pod 'JFWindguru/Pure', :path => '../'
  pod 'SCLAlertView', '~> 0.7'

  target 'JFWindguruPure_Tests' do
    inherit! :search_paths

    
  end
end

